{
    "extends": ["airbnb", "plugin:react/recommended", "plugin:lodash/recommended"],
    "rules": {
        "indent": ["error", 4, {"SwitchCase": 1}],
        "semi": ["error", "never"],
        "linebreak-style": "off",
        "react/require-default-props": "off",
        "lodash/import-scope": [2, "member"]

    },
    "plugins": [
        "react",
        "lodash"
    ],
    "parser": "babel-eslint"
}
