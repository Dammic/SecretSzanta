{
  "name": "SecretSzanta",
  "version": "0.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf public",
    "build:dll": "rimraf public && webpack --config webpack.dll.js",
    "build": "webpack --config webpack.dev.js",
    "build:watch": "webpack --config webpack.dev.js --watch --progress",
    "run": "node --experimental-modules server.mjs",
    "run:dev": "nodemon --experimental-modules server.mjs --watch utils/ --watch events/ --watch server.js",
    "dev": "npm-run-all --parallel build:watch run:dev",
    "eslint": "eslint ./src",
    "build:prod": "rimraf public && webpack --env prod --config webpack.dev.js",
    "test": "node_modules/jest/bin/jest.js",
    "test:fe": "node_modules/jest/bin/jest.js src/*",
    "test:silent": "NODE_HIDECONSOLE=true npm run test"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "packages(.*)$": "<rootDir>/src/packages$1"
    },
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ],
    "transform": {
      ".mjs": "babel-jest",
      ".js": "babel-jest"
    },
    "setupFiles": [
      "raf/polyfill",
      "./setupTests.js"
    ]
  },
  "authors": [
    "Damian Michalski",
    "Stanis≈Çaw Dawidowicz",
    "Marek Tkaczyk"
  ],
  "dependencies": {
    "classnames": "2.2.5",
    "font-awesome": "4.7.0",
    "history": "4.7.2",
    "lodash": "4.17.10",
    "moment": "2.22.1",
    "normalize.css": "7.0.0",
    "prop-types": "15.5.10",
    "react": "16.3.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.3.2",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "5.0.0-alpha.8",
    "react-transition-group": "2.3.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-thunk": "2.2.0",
    "socket.io": "1.7.3",
    "socket.io-client": "1.7.3"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.47",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.52",
    "@babel/preset-env": "7.0.0-beta.47",
    "@babel/preset-react": "7.0.0-beta.47",
    "autoprefixer": "6.7.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "brotli-webpack-plugin": "0.5.0",
    "compression-webpack-plugin": "1.1.3",
    "css-loader": "0.26.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.15.3",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-lodash": "2.4.3",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-standard": "2.0.1",
    "express": "4.14.1",
    "express-static-gzip": "0.3.2",
    "faker": "4.1.0",
    "file-loader": "0.10.1",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "3.4.2",
    "jest": "23.3.0",
    "merge-stream": "1.0.1",
    "node-sass": "4.9.0",
    "nodemon": "1.12.1",
    "npm-run-all": "4.0.2",
    "optipng-bin": "4.0.0",
    "pngquant-bin": "3.1.1",
    "postcss": "6.0.20",
    "postcss-cli": "5.0.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.2",
    "react-test-renderer": "16.3.2",
    "regenerator-runtime": "0.11.1",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.2",
    "uglifyjs-webpack-plugin": "1.2.2",
    "units-css": "0.4.0",
    "url-loader": "1.1.0",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.1"
  }
}
