{
  "name": "SecretSzanta",
  "version": "0.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "run": "node -r esm server/server.js",
    "run:dev": "nodemon server/server.js --watch server/utils/ --watch server/events/ --watch server/server.js",
    "eslint": "eslint ./src",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "test:fe": "jest src/*"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run build:prod & yarn run test"
    }
  },
  "authors": [
    "Damian Michalski",
    "StanisÅ‚aw Dawidowicz",
    "Marek Tkaczyk"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "classnames": "2.2.6",
    "connected-react-router": "^6.6.1",
    "customize-cra": "^0.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^23.0.5",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^6.0.0",
    "esm": "^3.2.25",
    "express": "4.17.1",
    "express-static-gzip": "2.0.5",
    "font-awesome": "4.7.0",
    "history": "4.10.1",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "node-sass": "4.13.1",
    "nodemon": "2.0.1",
    "postcss-import": "^12.0.1",
    "prop-types": "15.7.2",
    "raf-polyfill": "^1.0.0",
    "react": "^16.12.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-scripts": "3.2.0",
    "react-transition-group": "4.3.0",
    "redux": "4.0.4",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "2.3.0",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0"
  },
  "devDependencies": {
    "faker": "4.1.0",
    "husky": "^3.1.0",
    "react-test-renderer": "16.12.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.js",
      "packages(.*)$": "<rootDir>/src/packages$1"
    },
    "transform": {
      ".js": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./src/config/test.config.js"
    ]
  }
}
